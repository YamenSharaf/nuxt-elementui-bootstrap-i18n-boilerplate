{
  "name": "nuxt-elementui-bootstrap-i18n-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate ready to start developing Vue apps using Nuxt/ Element-UI/ Vuex/ Bootstrap/ i18n. Express.js also integrated to this can be a full stack app as well",
  "author": "Yamen Sharaf",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free-brands": "^5.0.6",
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/font-awesome": "^1.0.3",
    "aos": "^2.2.0",
    "bootstrap": "^4.0.0",
    "bootstrap-vue": "^2.0.0-rc.1",
    "element-ui": "^2.2.0",
    "express": "^4.15.3",
    "nuxt": "^1.0.0",
    "vue-i18n": "^7.4.2",
    "vue2-animate": "^1.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "cross-env": "^5.0.1",
    "eslint": "^4.15.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.11.0",
    "sass-loader": "^6.0.6"
  }
}
